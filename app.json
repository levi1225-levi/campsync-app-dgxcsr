{
  "expo": {
    "name": "CampSync",
    "slug": "CampSync",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/image-removebg-preview (3).png",
    "scheme": "CampSync",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/image-removebg-preview (3).png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.superlevi4566.campsync",
      "buildNumber": "32",
      "icon": "./assets/images/image-removebg-preview (3).png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NFCReaderUsageDescription": "This app needs access to NFC to scan and write to camper wristbands for check-in and identification."
      },
      "splash": {
        "image": "./assets/images/image-removebg-preview (3).png",
        "resizeMode": "contain",
        "backgroundColor": "#6366F1"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/image-removebg-preview (3).png",
        "backgroundColor": "#6366F1"
      },
      "icon": "./assets/images/image-removebg-preview (3).png",
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively",
      "versionCode": 32,
      "permissions": [
        "android.permission.NFC"
      ],
      "splash": {
        "image": "./assets/images/image-removebg-preview (3).png",
        "resizeMode": "contain",
        "backgroundColor": "#6366F1"
      }
    },
    "web": {
      "favicon": "./assets/images/image-removebg-preview (3).png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "This app needs access to NFC to scan and write to camper wristbands for check-in and identification.",
          "includeNdefEntitlement": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}